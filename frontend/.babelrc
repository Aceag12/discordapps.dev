{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "entry"
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    [
      "react-css-modules",
      {
        "removeImport": true
      }
    ]
  ],
  "env": {
    "node": {
      "plugins": [
        "react-hot-loader/babel",
        [
          "babel-plugin-transform-require-ignore",
          {
            "extensions": [".scss"]
          }
        ],
        [
          "babel-plugin-import-static-files",
          {
            "baseDir": "/",
            "outDir": "./frontend/build/"
          }
        ]
      ]
    }
  }
}
